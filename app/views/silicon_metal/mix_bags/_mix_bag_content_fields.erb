<%#*<script type="text/javascript">%>
  <%#*$(function() {%>
    <%#*$('.datePicker').each(function() {%>
      <%#*$(this).datepicker({%>
        <%#*maxDate: "+0D",%>
        <%#*firstDay: 1,%>
