<h1>Listing silicashiplots</h1>

<table>
  <tr>
    <th>Lot</th>
    <th>Weight</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @silicashiplots.each do |silicashiplot| %>
  <tr>
    <td><%= silicashiplot.lot %></td>
    <td><%= silicashiplot.weight %></td>
    <td><%= link_to 'Show', silicashiplot %></td>
    <td><%= link_to 'Edit', edit_silicashiplot_path(silicashiplot) %></td>
    <td><%= link_to 'Destroy', silicashiplot, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

